light_is_on = False

if light_is_on:
    print("The light is on!")
else:
    print("We're in the dark")
    
weight = 70

if weight < 65 :
   print("You're under weight")
if weight > 68 :
    print("you're above normal weight")
    

age = 22

if age >= 18 :
    print("Adult")
else:
    print("Child")
